{
  "version": "1.0.0-*",

  "buildOptions": {
    "embed": [ "Data/*.json" ]
  },

  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "System.Reflection.Extensions": "4.3.0",
    "Newtonsoft.Json": "9.0.1",
    "Humanizer.xproj": "2.1.0",
    "Microsoft.Extensions.DependencyModel": "1.1.0",
    "dotnet-test-mstest": "1.1.2-preview",
    "MsTest.TestFramework": "1.0.8-rc2",
    "JsonApiNet": {"target": "project", "version": "1.0.0-*"}
  },

  "testRunner": "mstest",

  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.1"
        }
      },
      "imports": [
        "dotnet5.4",
        "portable-net451+win8"
      ]
    }
  },

  "tooling": {
    "defaultNamespace": "JsonApiNet.Tests"
  }
}
